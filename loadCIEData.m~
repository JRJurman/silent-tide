function [ cie ] = loadCIEData()
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here

cie2deg = load('CIE_2Deg_380-780-5nm.txt');
cie10deg = load('CIE_10Deg_380-780-5nm.txt');

cie2t = cie2deg';
cie10t = cie10deg';

cie.lambda = cie2t(1,:);
cie.cmf2deg = cie2t(2:end,:)';
cie.cmf10deg = cie10t(2:end,:)';
cie.illA = load('CIE_IllA_380-780-5nm.txt');
cie.illA = cie.illA(:,2);
cie.illC = load('CIE_IllC_380-780-5nm.txt');
cie.illC = cie.illC(:,2);
cie.illD50 = load('CIE_IllD50_380-780-5nm.txt');
cie.
cie.illD65 = [];
cie.illE = [];
cie.illF = [];
cie.eigD = [];

end

